#include "fmncv_version.h"

namespace FMNCV
{
    std::string getFMNCVVersion()
    {
        return "@FMNCV_VERSION@";
    }

    unsigned int getFMNCVVersionMajor()
    {
        return @FMNCV_VERSION_MAJOR@;
    }
    
    unsigned int getFMNCVVersionMinor()
    {
        return @FMNCV_VERSION_MINOR@ + 0;
    }
    
    unsigned int getFMNCVVersionPatch()
    {
        return @FMNCV_VERSION_PATCH@ + 0;
    }
    
    unsigned int getFMNCVVersionTweak()
    {
        return @FMNCV_VERSION_TWEAK@ + 0;
    }
    
    std::string getFMNCVGitHash()
    {
        return "@FMNCV_GIT_HASH@";
    }
}


